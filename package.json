{
  "author": "gadhagod",
  "bin": {
    "imsg": "./bin/run.js"
  },
  "dependencies": {
    "@langchain/openai": "^0.0.14",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "langchain": "^0.1.12",
    "prompt-sync": "^4.2.0",
    "sqlite3": "5.1.7",
    "usearch": "^1.1.1"
  },
  "description": "Ask GPT questions about your iMessage history",
  "devDependencies": {
    "@types/node": "^18",
    "oclif": "^4.7.6",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/gadhagod/iMessageGPT",
  "license": "MIT",
  "main": "",
  "name": "imessagegpt",
  "oclif": {
    "bin": "imsg",
    "dirname": "iMessageGPT",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "config": {
        "description": "Manage configuration"
      },
      "chat": {
        "description": "Scan and query chats"
      }
    }
  },
  "repository": "gadhagod/iMessageGPT",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepare": "npm run build",
    "version": "npx oclif readme && git add README.md"
  },
  "version": "0.0.1",
  "bugs": "https://github.com/gadhagod/iMessageGPT/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
